{
  "resourceType": "Patient",
  "id": "${patientResourceId}",
  "identifier": [
    {
      "value": "${patientIdentifier}"
    }
  ],
  "active": true,
  "name": [
    {
      "use": "official",
      "family": "${patientFamilyName}",
      "given": [
        "${patientGivenName}"
      ]
    }
  ],
  "gender": "${patientGender}",
  "birthDate": "${patientBirthDateUpdated}"
}